{
  "author": "Red Badger",
  "dependencies": {
    "normalize.css": "^8.0.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "styled-components": "^4.4.1",
    "styled-normalize": "^8.0.6"
  },
  "description": "MHRA Medicine Information Portal",
  "devDependencies": {
    "@svgr/cli": "^4.3.3",
    "@svgr/webpack": "^4.3.3",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/node": "^12.12.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/styled-components": "^4.4.0",
    "@types/webpack": "^4.39.8",
    "@types/webpack-dev-server": "^3.4.0",
    "css-loader": "^3.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.3",
    "pa11y": "^5.3",
    "prettier": "^1.19.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.1.0",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,json,yml,md}": [
      "yarn prettier --write"
    ],
    "*.{ts,tsx}": [
      "yarn lint"
    ]
  },
  "main": "index.js",
  "name": "mhra-mip-website",
  "private": false,
  "scripts": {
    "lint": "yarn tslint",
    "start:dev": "webpack-dev-server --config ./config/webpack.config.dev.ts",
    "build": "webpack --config ./config/webpack.config.prod.ts",
    "a11y": "yarn run pa11y file://$PWD/dist/index.html"
  },
  "version": "1.0.0"
}
